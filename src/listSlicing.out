
> let lst = ['a' .. 'g'];;
val lst: char list = ['a'; 'b'; 'c'; 'd'; 'e'; 'f'; 'g']

> lst.[0];;
val it: char = 'a'

> lst.[3];;
val it: char = 'd'

> lst.[3..];;
val it: char list = ['d'; 'e'; 'f'; 'g']

> lst.[..3];;
val it: char list = ['a'; 'b'; 'c'; 'd']

> lst.[1..3];;
val it: char list = ['b'; 'c'; 'd']

> lst.[*];;
val it: char list = ['a'; 'b'; 'c'; 'd'; 'e'; 'f'; 'g']
